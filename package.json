{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "The goal is to create a standard dev environment for new js projects utalizing best practices.",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node scripts/start.js",
    "start": "run-p lint:watch test start-mockapi dev",
    "dev": "webpack-dev-server --config webpack.config.dev.babel.js --open --inline --hot",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",

    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "run-p localtunnel start",
    "lint": "esw webpack.config.* src scripts --color",
    "lint:watch": "npm run lint -- --watch",
    "generate-mock-data": "babel-node scripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "prebuild": "run-s test lint",
    "prod": "webpack --config webpack.config.prod.babel.js",
    "build": "babel-node scripts/build.js",
    "postbuild": "babel-node scripts/server.js"
  },
  "keywords": [],
  "author": "John Wyss",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.11",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/jest": "^23.3.8",
    "@types/webpack-env": "^1.13.6",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chalk": "^2.4.1",
    "cheerio": "^1.0.0-rc.2",
    "clean-webpack-plugin": "^0.1.19",
    "compression": "^1.7.3",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "csv-loader": "^3.0.2",
    "eslint": "^5.6.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jsdom": "^12.1.0",
    "json-schema-faker": "^0.5.0-rc16",
    "json-server": "^0.14.0",
    "localtunnel": "^1.9.1",
    "mini-css-extract-plugin": "^0.4.3",
    "nock": "^10.0.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "nsp": "^3.2.1",
    "numeral": "^2.0.6",
    "open": "^0.0.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-md5-hash": "0.0.6",
    "xml-loader": "^1.2.1"
  }
}
